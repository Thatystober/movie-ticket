// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Format {
  id          Int     @id @default(autoincrement())
  formatTitle String
  movies      Movie[]
}

model Movie {
  id       Int      @id @default(autoincrement())
  image    String
  title    String
  type     String
  duration String
  rank     String
  formats  Format[]
}
